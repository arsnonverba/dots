# wireguard
alias wgup="sudo wg-quick up us142"
alias wgdn="sudo wg-quick down us142"

# navigation aliases
alias www="cd ~/www/"
alias art="cd ~/art/"
alias ux="cd ~/ux/"

# alies fot git dotfiles
alias dots='/usr/bin/git --git-dir=$HOME/.dots/ --work-tree=$HOME'

#add cargo bin to path
export PATH="$HOME/.cargo/bin:$PATH"
